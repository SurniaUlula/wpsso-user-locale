#: lib/config-gettext-extract.php:3
msgctxt "lib file description"
msgid "User Locale"
msgstr "Langue utilisateur"

#: lib/config-gettext-extract.php:4
msgctxt "plugin description"
msgid "WPSSO extension to add a user locale / language selector in the WordPress admin back-end and front-end toolbar menus."
msgstr "Extension WPSSO pour ajouté un sélecteur de langue pour les utilisateurs dans la barre d'outils WordPress d'avant et arrière."

#: lib/filters.php:48
msgid "User Locale (%s)"
msgstr "Langue utilisateur (%s)"

#: lib/filters.php:73
msgid "A title string used for the WordPress toolbar menu. The \"%s\" parameter is replaced by the current user locale value."
msgstr "Le titre utilisé dans le menu de barre d’outils WordPress. Le paramètre \"%s\" est remplacé par la valeur actuel de la langue utilisateur."

#: lib/filters.php:76
msgid "Add the user locale selector to the front-end toolbar menu, and define current WordPress locale as the user locale value."
msgstr "Ajouter le sélecteur de langue utilisateur dans le menu de barre d'outils d'avant, et définir la langue courante WordPress comme celle de la langue utilisateur."

#: lib/locale.php:104
msgid "default"
msgstr "défaut"

#: lib/locale.php:127
msgid "Default Locale"
msgstr "Langue par défaut"

#: lib/submenu/ul-general.php:28
msgctxt "metabox title"
msgid "User Locale Settings"
msgstr "Réglages de langue utilisateur"

#: lib/submenu/ul-general.php:44
msgctxt "option label"
msgid "Toolbar Menu Title"
msgstr "Titre dans la barre d'outils"

#: lib/submenu/ul-general.php:48
msgctxt "option label"
msgid "Add User Locale on Front-End"
msgstr "Ajouter langue utilisateur à l'avant"

#: wpsso-user-locale.php:92
msgid "%1$s is an extension for the %2$s plugin &mdash; please install and activate the %3$s plugin before activating the %4$s extension."
msgstr "%1$s est une extension pour %2$s &mdash; s'il vous plaît installer et activer %3$s avant d'essayer de re-activer l'extension %4$s."

#: wpsso-user-locale.php:94
msgid "The %1$s extension requires the %2$s plugin &mdash; please install and activate the %3$s plugin."
msgstr "L'extension %1$s requiert %2$s &mdash; s'il vous plaît installer et activer l'extension %3$s."

#: wpsso-user-locale.php:107
msgid "%1$s requires WordPress version %2$s or higher and has been deactivated."
msgstr "%1$s nécessite WordPress version %2$s ou supérieure et a été désactivé."

#: wpsso-user-locale.php:109
msgid "Please upgrade WordPress before trying to reactivate the %1$s plugin."
msgstr "Veuillez mettre à jour WordPress avant d'essayer de réactiver l'extension %1$s."

#: wpsso-user-locale.php:173
msgid "The %1$s extension v%2$s requires %3$s v%4$s or newer (v%5$s currently installed)."
msgstr "L'extension %1$s v%2$s requiert l'utilisation de %3$s v%4$s ou plus récente (v%5$s est actuellement installée)."

